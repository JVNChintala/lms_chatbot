# Canvas LMS Configuration
CANVAS_URL=https://your-canvas-instance.com/api/v1
CANVAS_TOKEN=your_canvas_api_token_here

# Disable LiteLLM telemetry
LITELLM_TELEMETRY=False

# Gemini API Key (optional)
GEMINI_API_KEY=your_gemini_api_key_here

# Ollama Configuration - Optimized for Qwen2.5 7B Tool Calling
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=qwen2.5:7b

# Qwen2.5 Performance Optimization
OLLAMA_TEMPERATURE=0.1
OLLAMA_TOP_P=0.9
OLLAMA_TOP_K=40
OLLAMA_REPEAT_PENALTY=1.05
OLLAMA_NUM_CTX=8192
OLLAMA_NUM_PREDICT=512
OLLAMA_TIMEOUT=120

# Tool Calling Optimization
TOOL_CALLING_ENABLED=true
TOOL_DETECTION_TEMPERATURE=0.05
TOOL_EXECUTION_TEMPERATURE=0.2
MAX_TOOL_ITERATIONS=3