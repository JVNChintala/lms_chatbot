# Canvas LMS Configuration
CANVAS_URL=https://your-canvas-instance.com/api/v1
CANVAS_TOKEN=your_canvas_api_token

# AI Inference Systems (OpenAI is default and recommended)
# OpenAI Configuration (PRIMARY - Fast & Reliable)
OPENAI_API_KEY=your_openai_api_key

# Alternative AI Systems (OPTIONAL)
# Google Gemini Configuration
GEMINI_API_KEY=your_gemini_api_key

# AWS Bedrock Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# AI Video Generators (configure at least one for video features)
SYNTHESIA_API_KEY=your_synthesia_api_key
RUNWAY_API_KEY=your_runway_api_key
STABILITY_API_KEY=your_stability_api_key

# Model Configuration (optional - uses defaults if not set)
OPENAI_MODEL=gpt-4o-mini
GEMINI_MODEL=gemini-pro
BEDROCK_MODEL=anthropic.claude-3-sonnet-20240229-v1:0
MAX_TOKENS=1000
TEMPERATURE=0.7

# Application Settings
SECRET_KEY=your-secret-key-here
DEBUG=True